<div class="pagination">
    <ul>
        <% for(let i=1;i<=pages;i++) {%>
            <% if(i==currentPage){-%>
                <li  class="current"><%= i %></li>
           <% } else { -%> 
           
            <li><a class="blog-pagination"><%= i %></a></li>
                <% }-%>
        <% } %>  
    </ul>
</div>